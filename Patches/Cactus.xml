<Patch>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Plant_SaguaroCactus"]/label</xpath>
    <value>
      <label>cactus</label>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Plant_SaguaroCactus"]/graphicData</xpath>
    <value>
      <graphicData>
        <graphicClass>Graphic_Single</graphicClass>
        <texPath>Blocky/Trees/Cactus</texPath>
        <shadowData>
          <volume>(0.2, 0.35, 0.13)</volume>
          <offset>(-0.03,0,-0.44)</offset>
        </shadowData>
      </graphicData>
    </value>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Plant_SaguaroCactus"]/plant</xpath>
    <value>
      <visualSizeRange>1.0~2.0</visualSizeRange>
      <choppedThingDef IsNull="True" />
      <immatureGraphicPath>Blocky/Trees/Cactus_Immature</immatureGraphicPath>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Plant_SaguaroCactus"]/plant/leaflessGraphicPath</xpath>
    <value>
      <leaflessGraphicPath>Blocky/Trees/Cactus_Leafless</leaflessGraphicPath>
    </value>
  </Operation>
</Patch>
